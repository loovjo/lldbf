#include hexdigit.bfpp
#include divmod.bfpp

(? >input denom div_res tmp2 tmp1 mod_res)

inv add16(denom input)

(!input) , [
    inv divmod(input denom div_res mod_res tmp1 tmp2)

    inv digit2hex(div_res tmp1 tmp2)
    (!div_res) . [-]

    inv digit2hex(mod_res tmp1 tmp2)
    (!mod_res) . [-]

    inv add32(tmp2 tmp1)
    (!tmp2) . [-]

    (!input) ,
]
